    "from collections import deque\n",
    "k=4\n",
    "score = [0, 300, 40, 300, 20, 70, 150, 50, 500, 1000]\n",
    "q = []\n",
    "result=[]\n",
    "for i in score:\n",
    "    q.append(i)\n",
    "    if len(q) > k:\n",
    "        q.remove(min(q))\n",
    "    print(q)\n",
    "    result.append(min(q))\n",
    "print(result)"
   ]
